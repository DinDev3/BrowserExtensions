{

    "manifest_version": 2,
    "name": "Magnify",
    "version": "1.0",
  
    "description": "Magnifies the body content of all webpages matching mozilla.org.",
  
    "icons": {
      "48": "icons/border-48.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.mozilla.org/*"],
        "js": ["magnify.js"]
      }
    ]
  
  }